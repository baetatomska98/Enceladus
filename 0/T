FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];


internalField   uniform 270;

boundaryField
{
    inlet
    {
        type            totalTemperature;
        gamma           1.333;
        T0              uniform 273.16;
        value           uniform 273.16;
    }
    outlet
    {
        type            zeroGradient;
    }
    outerwall
    {
        type            zeroGradient;
    }
    longitudinal_symmetry
    {
        type            symmetryPlane;
    }
    lateral_sides
    {
        type            empty;
    }
}


